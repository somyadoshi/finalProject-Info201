max_lng <- max(usa_ufo_data$lng)
lng_diff <- max_lng - min_lng
west_cutoff <- min_lng + lng_diff/3
east_cutoff <- max_lng - lng_diff/3
### the server portion of the app
server <- function( input , output ) {
### creating the user based filtered set of information based on region and shape
user_interest <- reactive({
if(input$loc == "west"){
user_interest <- filter( usa_ufo_data , as.double(usa_ufo_data$lng) < west_cutoff)
} else if(input$loc == "east"){
user_interest <- filter( usa_ufo_data , as.double(usa_ufo_data$lng) > east_cutoff)
} else if(input$loc == "middle") {
user_interest <- filter( usa_ufo_data , as.double(usa_ufo_data$lng) >= west_cutoff)
user_interest <- filter( user_interest , as.double(user_interest$lng) <= east_cutoff)
} else{
user_interest <- usa_ufo_data
}
user_interest <- filter(user_interest,user_interest$Shape==input$shape)
})
### renders the visualization, a scatterplot based on user interest (latitude or time)
output$scatter <- renderPlot({
if(input$analysis=="lat"){
y <- user_interest$lat
return( ggplot(data = usa_ufo_data) +
geom_point(mapping = aes(x=usa_ufo_data$lat , y=usa_ufo_data$Shape) ) +
geom_bin2d(mapping = aes(x=usa_ufo_data$lat , y=usa_ufo_data$Shape) ) )
} else{
y <- user_interest$Time
return( ggplot(data = usa_ufo_data) +
geom_point(mapping = aes(x=usa_ufo_data$Time , y=usa_ufo_data$Shape) ) +
geom_bin2d(mapping = aes(x=usa_ufo_data$Time , y=usa_ufo_data$Shape) ) )
}
})
### finds the most frequently sighted object
output$msg <- renderText({
shape_summary <- group_by(usa_ufo_data, Shape) %>% summarize(count = n())
most_shapes <- max(shape_summary$count)
most_frequent_shape <- filter(shape_summary, count == most_shapes)$Shape
message <- paste("The most frequently sighted shape was",most_frequent_shape)
return(message)
})
}
